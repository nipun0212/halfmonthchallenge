service cloud.firestore {
  match /databases/{database}/documents {
    match /Organizations/{organizationId}{
    			allow create:if request.auth.token.role=="superAdmin"	||
          request.auth.token.organizationId ==organizationId
          allow read:if request.auth.token.role=="superAdmin"	||
          request.auth.token.organizationId ==organizationId
          allow update:if request.auth.token.role=="superAdmin"	||
          request.auth.token.organizationId ==organizationId
           allow write:if request.auth.token.role=="superAdmin"	||
          request.auth.token.organizationId ==organizationId
          allow get:if request.auth.token.role=="superAdmin"	||
          request.auth.token.organizationId ==organizationId
        }
    }
}